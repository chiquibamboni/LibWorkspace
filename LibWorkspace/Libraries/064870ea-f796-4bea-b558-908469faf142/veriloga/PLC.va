`include "constants.h"
`include "disciplines.h"

module PLC (p1, p2);
    inout p1, p2;
    electrical p1, p2;

    // Параметры компонентов
    parameter real L = 1n;
    parameter real C = 1p;

    analog begin
     I(p1, p2) <+ C * ddt(V(p1, p2));
     V(p1, p2) <+ L * ddt(I(p1, p2));
  
  end
endmodule
